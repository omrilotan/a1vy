{
  "source": {
    "include": [],
    "exclude": [],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/summarize"
  ],
  "templates": {
    "default": {
      "includeDate": false
    }
  },
  "opts": {
    "mainpagetitle": "Documentation",
    "readme": "README.md",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true
  }
}
